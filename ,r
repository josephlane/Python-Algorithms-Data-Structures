import math

def binary_search(array, start, end, value):

    if start > end:
        return False

    mid = math.floor((start+ end) / 2)

    if value < array[mid]:
        return binary_search(array, start, mid)
    else:
        return binary_search(array, mid, end)

    return False


a = [1,2,3,4,5,6:

print binary_search(a, 0, len(a)-1, 3)
